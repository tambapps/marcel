import static org.junit.jupiter.api.Assertions.assertEquals

class A1 {
  int i
  int j
  constructor(this.i, this.j)
}

class A2 {
  int i
  int j
  protected constructor(int i, int j) {
    this.i = i
    this.j = j
  }
}

class B extends A1 {
  int k
  constructor(int i, int j, this.k): super(i, j)
}

A1 a1 = new A1(1, 2)
assertEquals(1, a1.i)
assertEquals(2, a1.j)

A2 a2 = new A2(1, 2)
assertEquals(1, a2.i)
assertEquals(2, a2.j)

B b = new B(1, 2, 3)
assertEquals(1, b.i)
assertEquals(2, b.j)
assertEquals(3, b.k)

