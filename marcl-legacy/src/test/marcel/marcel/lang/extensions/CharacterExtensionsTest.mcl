package marcel.lang.extensions

import org.junit.jupiter.api.Test

import static org.junit.jupiter.api.Assertions.assertEquals

class CharacterExtensionsTest {

  @Test
  fun void testCharUpperCase() {
    char c = `a`
    assertEquals(`A`, c.toUpperCase())
  }

  @Test
  fun void testCharacterUpperCase() {
    Character c = `a`
    assertEquals(`A`, c.toUpperCase())
  }

}
